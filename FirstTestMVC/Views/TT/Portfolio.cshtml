var perfNumbers = function () {
    for (i = 1; i <= 10000; i++) {
        var factors = [];
        if (i === 1) {
            factors.push(1);
        };
        for (var j = 1; j < i; j++) {
            var remainder = i % j;
            if (remainder === 0) {
                factors.push(j);
            };
        }
        numIndex = factors.length - 1;
        var answer = 0;
        function multiple(index) {
            if (index < 0) {
                return;
            }
            answer = answer + factors[index];
            return multiple(index - 1);
        };
        multiple(numIndex);
        //console.log(answer);
        if (i === 1) {
        }
        else if (answer === i) {
            document.getElementById('displayPerfNums')
                    .textContent +="\n" + "The number "
                     + i + " is a perfect number!" +"\n";
        }
    }
}
//Takes a number entered by the user and determines if
//it is a perfect number.
